{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [ "^build" ],
      "inputs": [ "$TURBO_DEFAULT$", ".env*" ],
      "outputs": [ ".next/**", "!.next/cache/**" ]
    },
    "lint": {
      "dependsOn": [ "^lint" ]
    },
    "check-types": {
      "dependsOn": [ "^check-types" ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "migrate:dev": {
      "cache": false
    },
    "migrate:deploy": {
      "cache": false
    }
  }
}
